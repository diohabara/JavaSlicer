class A {

    int xA = 0;

    A(int newx) {
        xA = newx;
    }

    int getx() {
        return xA;
    }

    public static void main(String[] args) {
        B b1 = new B(5.6);
        int z = b1.getx();
        System.out.println(z);
    }
}

class B extends A {

    B(double valor) {
        super((int) valor);
    }
}
